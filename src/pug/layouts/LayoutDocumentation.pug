extends LayoutTheme
include ../mixins/common/TopBar
include ../mixins/common/PreFooter
include ../mixins/common/Footer
include ../mixins/navbar/Navbar
include ../mixins/navbar/NavbarDocs
include ../mixins/common/PageHeader
include ../mixins/common/SectionHeader
include ../mixins/common/Member

append variables 
  - var pageHeaderConfig = {}

prepend styles
  link(href=`${CWD}vendors/prism/prism.css`, rel='stylesheet')

prepend scripts
  script(src=`${CWD}vendors/prism/prism.js`)

block beforeMainContent
  +TopBar
  +Navbar
  +PageHeader(pageHeaderConfig)

append mainContent
  +Section 
    .row.justify-content-between 
      .d-none.d-lg-block.col-3.mb-5 
        .sticky-sidebar.pt-3.z-index-1
          +NavbarDocs
      .col-lg-9.py-3
        block content

block afterMainContent
  +PreFooter
  +Footer
